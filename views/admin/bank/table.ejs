<div class="card shadow mb-4">
    <div class="card-header py-3">
      <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#add-modal">
        <i class="fas fa-plus"></i>
       </button>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>No</th>
              <th>Nama Bank</th>
              <th>Nomer Rekening</th>
              <th>Atas Nama</th>
             <th>image</th>
             <th>action</th>
            </tr>
          </thead>
    
          <tbody>
            <% for(let i = 0; i < bank.length; i++) {  %> 
              <tr>
                  <td><%= i + 1 %> </td>
                  <td><%= bank[i].nameBank  %> </td>
                  <td><%= bank[i].nomerRekening  %> </td>
                  <td><%= bank[i].name  %> </td>
                  <td><img src="https://server-staycation.herokuapp.com/<%= bank[i].imageUrl %>  " width="100" height=70 alt=""></td>
                  <td>
                    <a href="javascript:void()" type="button" class="btn btn-warning btn-circle btn-sm btn-update" data-id="<%= bank[i].id %>" data-name="<%= bank[i].name %>"  data-namebank="<%= bank[i].nameBank %> " data-nomerrekening="<%= bank[i].nomerRekening %>"><i class="fas fa-edit"></i></a>
                    <form class="d-inline" action="/admin/bank/<%= bank[i].id %>?_method=DELETE" method="POST">
                        <button type="submit" class="btn btn-danger btn-circle btn-sm"><i class="fas fa-trash"></i></button>

                      </form>
                     
  
                   
                  </td>

              </tr>
            <% } %> 
          </tbody>
        </table> 
      </div>
    </div>
  </div>

